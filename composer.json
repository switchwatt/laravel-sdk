{
    "name": "switchwatt/laravel-sdk",
    "description": "An SDK to interact with the B2B Energy Switching Platform.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Richie Pearce",
            "email": "richie@switchwatt.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/http": "^11.0 || ^12.0",
        "illuminate/support": "^11.0 || ^12.0"
    },
    "autoload": {
        "psr-4": {
            "Switchwatt\\LaravelSdk\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Switchwatt\\LaravelSdk\\LaravelSdkServiceProvider"
            ],
            "aliases": {
                "Platform": "Switchwatt\\LaravelSdk\\Facades\\Platform"
            }
        }
    }
}
